# kevin ci main config file

[kevin]
name=kevin-ci
max_jobs_queued=50
desc_file=.kevin
max_output=209715200
job_timeout=600
silence_timeout=60

[github]
user=[your fancy github username]
token=[your super secret token]
hooksecret=[your magically unknown hook secret]

# the job output folder
[web]
url=http://your.kevin.host.serving.static/maybe/subfolder
folder=/your/writable/static/web/folder/served/above
dyn_port=7777
dyn_url=http://your.kevin.host:7777/

# configure available falk instances
[falk]
falk0=falk@/run/kevin/falk
