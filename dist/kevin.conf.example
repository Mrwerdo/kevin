# kevin ci main config file

[kevin]
name = kevin-ci
max_jobs_queued = 50

[projects]
config_folder = /etc/kevin/projects
output_folder = /your/writable/static/web/folder/served/below

# the job output folder
[web]
url = http://your.kevin.host.serving/output_folder/statically
dyn_port = 7777
dyn_url = http://your.kevin.host:7777/

# configure available falk instances
[falk]
lol-unix-falk = yourmom@/run/kevin/falk
lol-ssh-falk = yourdad@your.falk.host:22
